/* pages/epidemic/epidemic.wxss */
.comeback{
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  z-index: 1;
  font-size: 22rpx;
  color: #0078d4;
}
.epidemic{
  height: 100%;
  background: rgba(16,174,181,0.7);
  display: flex;
  flex-direction: column;
  align-items: center;
}
.header{
  width: 100%;
  color: #c4faff;
  font-size: 38rpx;
  padding: 10rpx 0 20rpx 0;
  text-align: center;
}
.time{
  font-size: 28rpx;
  color: #999;
}
.content{
  height: 88%;
  width: 83%;
  padding: 20rpx;
  border-radius: 16rpx;
  background: #fff;
  overflow: hidden;
}
.collect{
  height: 100%;
  font-size: 24rpx;
  display: flex;
  flex-direction: column;
}
.collect-title{
  font-size: 26rpx;
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.collect-title text{
  text-align: center;
}
.collect-title .active {
  border-bottom: 4rpx solid rgba(16,174,181,1);
  color: #10aeb5;
  font-weight: 700;
}

china-panel{
  height: 90%;
}
